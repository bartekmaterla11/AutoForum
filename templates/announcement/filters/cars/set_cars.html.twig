    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Kolor</label>
        <div class="color-block ">
            <select id="color-cars-block" {% if filters.color is defined %} name="color" {% else %} name="" {% endif %}
                    class="categorySelect">
                <option value="0" selected>Wszystkie</option>
                {% for item in datas.color %}
                    <option {% if filters.color == item.id %} selected {% endif %}
                            value="{{ item.id }}">{{ item.name }}</option>
                {% endfor %}
            </select>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Stan techniczny</label>
        <div class="condition-block ">
            <select id="condition-cars-block" {% if filters.condition is defined %} name="condition" {% else %} name="" {% endif %}
                    class="categorySelect">
                <option value="0" selected>Wszystkie</option>
                {% for item in datas.condition %}
                    <option {% if filters.condition == item.id %} selected {% endif %}
                            value="{{ item.id }}">{{ item.name }}</option>
                {% endfor %}
            </select>
        </div>
    </div>
    <div style="clear: both"></div>

</div>