<div id="set-cars-form">
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Marka</label>
        <div class="mark-block ">
            <select id="markAnnounSelect" {% if filters.mark is defined %} name="mark" {% else %} name="" {% endif %}
                    class="w-100 categorySelect">
                <option class="categoryOption" value="0" selected>Wszystkie</option>
                {% for item in datas.markCars %}
                    <option {% if filters.mark == item.id %} selected {% endif %} class="categoryOption"
                                                                                  value="{{ item.id }}">{{ item.name }}</option>
                {% endfor %}
            </select>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Model</label>
        <div class="modelsAnnounSelect">
            <select id="modelAnnounSelect" {% if filters.model is defined %} name="model" {% else %} name="" {% endif %}
                    disabled class="w-100 categorySelect">
                <option class="categoryOption" value="0" selected>Wszystkie</option>
                {% for item in datas.markCars %}
                    {% for item in item.modelOfCars %}
                        <option {% if filters.model == item.id %} selected {% endif %} class="categoryOption"
                                                                                       value="{{ item.id }}">{{ item.name }}</option>
                    {% endfor %}
                {% endfor %}
            </select>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Cena (zł)</label>
        <div class="price-cars-block ">
            <div id="cars_price_from" class="float-left w-48">
                <input id="cars_price_from1"
                       placeholder="od" {% if filters.priceF is defined %} name="price_from" {% if filters.priceF > 0 %} value="{{ filters.priceF }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div id="cars_price_to" class="float-left w-48 ml-1">
                <input id="cars_price_to1"
                       placeholder="do" {% if filters.priceT is defined %} name="price_to" {% if filters.priceT != 99999999 %}  value="{{ filters.priceT }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div style="clear: both"></div>
        </div>
    </div>
    <div style="clear: both"></div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Rok produkcji</label>
        <div class="year-cars-block ">
            <div id="cars_year_from" class="float-left w-48">
                <input id="cars_year_from1"
                       placeholder="od" {% if filters.yearF is defined %} name="year_from" {% if filters.yearF > 0 %} value="{{ filters.yearF }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div id="cars_year_to" class="float-left w-48 ml-1">
                <input id="cars_year_to1"
                       placeholder="do" {% if filters.yearT is defined %} name="year_to" {% if filters.yearT != 9999 %} value="{{ filters.yearT }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div style="clear: both"></div>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Poj. silnika</label>
        <div class="capacity-cars-block ">
            <div id="cars_cap_from" class="float-left w-48">
                <input id="cars_cap_from1"
                       placeholder="od" {% if filters.capF is defined %} name="capacity_from" {% if filters.capF > 0 %} value="{{ filters.capF }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div id="cars_cap_to" class="float-left w-48 ml-1">
                <input id="cars_cap_to1"
                       placeholder="do" {% if filters.capT is defined %} name="capacity_to" {% if filters.capT != 999999 %} value="{{ filters.capT }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div style="clear: both"></div>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Przebieg</label>
        <div class="mileage-cars-block ">
            <div id="cars_mil_from" class="float-left w-48">
                <input id="cars_mil_from1"
                       placeholder="od" {% if filters.milF is defined %} name="mileage_from" {% if filters.milF > 0 %} value="{{ filters.milF }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div id="cars_mil_to" class="float-left w-48 ml-1">
                <input id="cars_mil_to1"
                       placeholder="do" {% if filters.milT is defined %} name="mileage_to" {% if filters.milT != 99999999 %} value="{{ filters.milT }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div style="clear: both"></div>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Moc silnika</label>
        <div class="power-cars-block ">
            <div id="cars_pow_from" class="float-left w-48">
                <input id="cars_pow_from1"
                       placeholder="od" {% if filters.powF is defined %} name="power_from" {% if filters.powF > 0 %} value="{{ filters.powF }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div id="cars_pow_to" class="float-left w-48 ml-1">
                <input id="cars_pow_to1"
                       placeholder="do" {% if filters.powT is defined %} name="power_to" {% if filters.powT != 99999 %} value="{{ filters.powT }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div style="clear: both"></div>
        </div>
    </div>
    <div style="clear: both"></div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Paliwo</label>
        <div class="fuel-block ">
            <select id="fuel-cars-block" {% if filters.fuel is defined %} name="fuel" {% else %} name="" {% endif %}
                    class="categorySelect">
                <option value="0" selected>Wszystkie</option>
                {% for item in datas.fuel %}
                    <option {% if filters.fuel == item.id %} selected {% endif %}
                            value="{{ item.id }}">{{ item.name }}</option>
                {% endfor %}
            </select>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
