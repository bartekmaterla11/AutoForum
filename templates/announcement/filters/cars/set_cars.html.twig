                    class="categorySelect">
                <option value="0" selected>Wszystkie</option>
                {% for item in datas.gearbox %}
                    <option {% if filters.gearbox == item.id %} selected {% endif %}
                            value="{{ item.id }}">{{ item.name }}</option>
                {% endfor %}
            </select>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Typ nadwozia</label>
        <div class="fuel-block ">
            <select id="body-cars-block" {% if filters.body is defined %} name="body" {% else %} name="" {% endif %}
                    class="categorySelect">
                <option value="0" selected>Wszystkie</option>
                {% for item in datas.body %}
                    <option {% if filters.body == item.id %} selected {% endif %}
                            value="{{ item.id }}">{{ item.name }}</option>
                {% endfor %}
            </select>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Kraj pochodzenia</label>
        <div class="country-block ">
            <select id="country-cars-block" {% if filters.country is defined %} name="country" {% else %} name="" {% endif %}
                    class="categorySelect">
                <option value="0" selected>Wszystkie</option>
                {% for item in datas.country %}
                    <option {% if filters.country == item.id %} selected {% endif %}
                            value="{{ item.id }}">{{ item.name }}</option>
                {% endfor %}
            </select>
        </div>
    </div>
    <div style="clear: both"></div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Kierownica</label>
        <div class="handlebar-block ">
            <select id="handlebar-cars-block" {% if filters.handlebar is defined %} name="handlebar" {% else %} name="" {% endif %}
                    class="categorySelect">
                <option value="0" selected>Wszystkie</option>
                {% for item in datas.handlebar %}
                    <option {% if filters.handlebar == item.id %} selected {% endif %}
