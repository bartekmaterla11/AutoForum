<div id="set-parts-cars-form">
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Rodzaj</label>
        <div class="type-parts-motors-block">
            <select id="type-parts-motors-block"  name=""
                    class="categorySelect">
                <option value="0" selected>Wszystkie</option>
                {% for item in typeParts %}
                    <option {% if filters.typeParts == item.id %} selected {% endif %}
                            value="{{ item.id }}">{{ item.name }}</option>
                {% endfor %}
            </select>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Cena (z≈Ç)</label>
        <div class="price-parts-motors-block ">
            <div id="parts_motors_price_from" class="float-left w-48">
                <input id="parts_motors_price_from1"
                       placeholder="od" {% if filters.priceF is defined %} name="price_from" {% if filters.priceF > 0 %} value="{{ filters.priceF }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div id="parts_motors_price_to" class="float-left w-48 ml-1">
                <input id="parts_motors_price_to1"
                       placeholder="do" {% if filters.priceT is defined %} name="price_to" {% if filters.priceT != 99999999 %}  value="{{ filters.priceT }}" {% endif %} {% else %} name="" {% endif %}
                       type="text" class="textType">
            </div>
            <div style="clear: both"></div>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Stan</label>
        <div class="state-parts-motors-block ">
            <select id="state-parts-motors-block" {% if filters.state is defined %} name="state" {% else %} name="" {% endif %}
                    class="categorySelect">
                <option value="0" selected>Wszystkie</option>
                {% for item in datas.state %}
                    <option {% if filters.state == item.id %} selected {% endif %}
                            value="{{ item.id }}">{{ item.name }}</option>
                {% endfor %}
            </select>
        </div>
    </div>
</div>