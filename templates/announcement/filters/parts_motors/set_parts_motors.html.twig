<form id="set-parts-cars-form" action="" method="get">
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Rodzaj</label>
        <div class="type-parts-motors-block">
            <select id="type-parts-motors-block" class="categorySelect">
                <option value="0" selected >Wszystkie</option>
                {% for item in type %}
                    <option value="{{ item.id }}">{{ item.name }}</option>
                {% endfor %}
            </select>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Cena (z≈Ç)</label>
        <div class="price-parts-motors-block ">
            <div class="float-left w-48">
                <input placeholder="od" type="text" list="items-price" class="textType">
            </div>
            <div class="float-left w-48 ml-1">
                <input placeholder="do" type="text" class="textType">
            </div>
            <div style="clear: both"></div>
        </div>
    </div>
    <div class="w-25 mb-2 float-left p-1">
        <label class="text-color-4">Stan</label>
        <div class="state-parts-motors-block ">
            <select id="state-parts-motors-block" class="categorySelect">
                <option value="0" selected >Wszystkie</option>
                {% for item in state %}
                    <option value="{{ item.id }}">{{ item.name }}</option>
                {% endfor %}
            </select>
        </div>
    </div>
    <div style="clear: both"></div>
    <div class="mt-3 w-100 text-right pr-2">
        <button type="submit" class="btn btn-outline-primary p-1 pl-3 pr-3 ">Filtruj</button>
    </div>
</form>